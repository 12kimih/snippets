{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Enumerate (a)": {
		"prefix": "\\enumerate",
		"body": ["\\begin{enumerate}[label=(\\alph*)]", "\t\\item $1", "\\end{enumerate}$0"]
	},

	"Enumerate i)": {
		"prefix": "\\enumerate",
		"body": ["\\begin{enumerate}[label=\\roman*)]", "\t\\item $1", "\\end{enumerate}$0"]
	},

	"IEEEeqnarray": {
		"prefix": "\\IEEEeqnarray",
		"body": ["\\begin{IEEEeqnarray}{rCl}", "\t$1", "\\end{IEEEeqnarray}$0"]
	},

	"IEEEeqnarray*": {
		"prefix": "\\IEEEeqnarray*",
		"body": ["\\begin{IEEEeqnarray*}{rCl}", "\t$1", "\\end{IEEEeqnarray*}$0"]
	},

	"IEEEeqnarraymulticol": {
		"prefix": "\\IEEEeqnarraymulticol",
		"body": ["\\IEEEeqnarraymulticol{3}{l}{", "\t$1", "}$0"]
	},

	"Align =": {
		"prefix": "=",
		"body": "& = & $0"
	},

	"Align Character": {
		"prefix": "\\&",
		"body": "& $1 & $0"
	},

	"Multiline Equation": {
		"prefix": "\\negmedspace",
		"body": "&& \\negmedspace {} $0"
	},

	"Definition Array": {
		"prefix": "\\defarray",
		"body": ["\\left\\{ \\begin{array}{cl}", "\t$1 & \\text{if }$2 \\\\\\", "\t$3 & \\text{if }$4", "\\end{array} \\right.$0"]
	},

	"Function Array": {
		"prefix": "\\funcarray",
		"body": ["\\left\\{ \\begin{array}{r@{\\;}c@{\\;}l}", "\t$1", "\\end{array} \\right.$0"]
	},

	"Roman": {
		"prefix": "\\mathrm",
		"body": "\\mathrm{$1}$0"
	},

	"Sum": {
		"prefix": "\\sum",
		"body": "\\sum_{$1}^{$2}$0"
	},

	"Integral": {
		"prefix": "\\int",
		"body": "\\int_{$1}^{$2}$3\\dd{$4}$0"
	},

	"Union": {
		"prefix": "\\bigcup",
		"body": "\\bigcup_{$1}^{$2}$0"
	},

	"Intersection": {
		"prefix": "\\bigcap",
		"body": "\\bigcap_{$1}^{$2}$0"
	},

	"Limit": {
		"prefix": "\\lim",
		"body": "\\lim_{$1}$0"
	},

	"Probability Limit": {
		"prefix": "\\plim",
		"body": "\\plim_{$1}$0"
	},

	"Auto-sized ()": {
		"prefix" : "()",
		"body": "\\qty($1)$0"
	},

	"Auto-sized {}": {
		"prefix" : "{}",
		"body": "\\qty{$1}$0"
	},

	"Auto-sized []": {
		"prefix" : "[]",
		"body": "\\qty[$1]$0"
	},

	"<>": {
		"prefix": "<>",
		"body": "\\langle $1 \\rangle$0"
	},

	"Evaluation": {
		"prefix": "\\eval",
		"body": "\\eval[$1|_{$2}^{$3}$0"
	},

	"Equation $$": {
		"prefix": "\\$",
		"body": "$$1$$0"
	},

	"Equation []": {
		"prefix" : "\\[]",
		"body": "\\[ $1 \\]$0"
	},

	"Subscript": {
		"prefix": "_{}",
		"body": "_{$1}$0"
	},

	"Text Subscript": {
		"prefix": "_\\text",
		"body": "_{\\text{$1}}$0"
	},

	"Superscript": {
		"prefix": "^{}",
		"body": "^{$1}$0"
	},

	"Complement": {
		"prefix": "\\complement",
		"body": "^{\\mathrm{c}}$0"
	},

	"Real Part": {
		"prefix": "\\Re",
		"body": "\\Re{$1}$0"
	},

	"Variance": {
		"prefix": "\\Var",
		"body": "\\mathrm{Var}\\qty($1)$0"
	},

	"Covariance": {
		"prefix": "\\Cov",
		"body": "\\mathrm{Cov}\\qty($1)$0"
	},

	"Correlation": {
		"prefix": "\\Corr",
		"body": "\\mathrm{Corr}\\qty($1)$0"
	},

	"for": {
		"prefix": "\\for",
		"body": "\\quad \\text{for }$0"
	},

	"because": {
		"prefix": "\\because",
		"body": "\\quad \\because $0"
	},

	"and": {
		"prefix": "\\and",
		"body": "\\text{ and }$0"
	},

	"subfigure": {
		"prefix": "\\subfigure",
		"body": ["\\begin{subfigure}{0.4\\textwidth}", "\t$1", "\t\\caption{$2}", "\t\\label{$3}", "\\end{subfigure}$0"]
	},

	"textwidth": {
		"prefix": "\\textwidth",
		"body": "\\textwidth$0"
	},

	"linewidth": {
		"prefix": "\\linewidth",
		"body": "\\linewidth$0"
	},

	"problem": {
		"prefix": "\\problem",
		"body": "\\textbf{\\cite{$1}$2}$0"
	},

	"mbox baselineskip": {
		"prefix": "\\mbox",
		"body": "\\mbox{} \\vspace{-\\baselineskip}$0"
	},

	"si": {
		"prefix": "\\si",
		"body": "\\si{$1}$0"
	},

	"SI": {
		"prefix": "\\SI",
		"body": "\\SI{$1}{$2}$0"
	},

	"SIrange": {
		"prefix": "\\SIrange",
		"body": "\\SIrange{$1}{$2}{$3}$0"
	},

	"Binomial": {
		"prefix": "\\binom",
		"body": "\\binom{$1}{$2}$0"
	},

	"lstlisting": {
		"prefix": "\\lstlisting",
		"body": ["\\begin{lstlisting}", "\t$1", "\\end{lstlisting}"]
	},

	"inline verbatim": {
		"prefix": "\\verb",
		"body": "\\verb|$1|$0"
	},

	"Fourier Transform": {
		"prefix": "\\FT",
		"body": "\\stackrel{\\mathcal{FT}}{\\longleftrightarrow}"
	},

	"Fourier Series": {
		"prefix": "\\FS",
		"body": "\\stackrel{\\mathcal{FS}}{\\longleftrightarrow}"
	},

	"Odd": {
		"prefix": "\\Od",
		"body": "\\mathrm{Od}\\qty{$1}$0"
	},

	"Even": {
		"prefix": "\\Ev",
		"body": "\\mathrm{Ev}\\qty{$1}$0"
	},

	"Absolute Value": {
		"prefix": "\\abs",
		"body": "\\abs{$1}$0"
	}
}
