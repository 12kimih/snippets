{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Enumerate (a)": {
		"prefix": "\\enumerate",
		"body": ["\\begin{enumerate}[label=(\\alph*)]", "\t\\item $1", "\\end{enumerate}$0"]
	},

	"Enumerate i)": {
		"prefix": "\\enumerate",
		"body": ["\\begin{enumerate}[label=\\roman*)]", "\t\\item $1", "\\end{enumerate}$0"]
	},

	"IEEEeqnarray": {
		"prefix": "\\IEEEeqnarray",
		"body": ["\\begin{IEEEeqnarray}{rCl}", "\t$1", "\\end{IEEEeqnarray}$0"]
	},

	"IEEEeqnarray*": {
		"prefix": "\\IEEEeqnarray*",
		"body": ["\\begin{IEEEeqnarray*}{rCl}", "\t$1", "\\end{IEEEeqnarray*}$0"]
	},

	"IEEEeqnarraymulticol": {
		"prefix": "\\IEEEeqnarraymulticol",
		"body": ["\\IEEEeqnarraymulticol{3}{l}{", "\t$1", "}$0"]
	},

	"Align =": {
		"prefix": "=",
		"body": "& = &$0"
	},

	"Multiline Equation": {
		"prefix": "\\negmedspace",
		"body": "&& \\negmedspace {}$0"
	},

	"Definition Array": {
		"prefix": "\\defarray",
		"body": ["$1 = \\left\\{ \\begin{array}{cl}", "\t$2 & \\text{if } $3 \\\\\\", "\t$4 & \\text{if } $5", "\\end{array} \\right.$0"]
	},

	"Roman Function": {
		"prefix": "\\mathrm",
		"body": "\\mathrm{$1} \\qty($2)$0"
	},

	"Probability": {
		"prefix": "\\Pr",
		"body": "\\Pr($1)$0"
	},

	"Expectation": {
		"prefix": "\\E",
		"body": "\\mathrm{E} \\qty($1)$0"
	},

	"Variance": {
		"prefix": "\\Var",
		"body": "\\mathrm{Var} \\qty($1)$0"
	},

	"Indication Function": {
		"prefix": "\\I",
		"body": "\\mathrm{I}_{$1} \\qty($2)$0"
	},

	"Cumulative Distribution Function": {
		"prefix": "\\F",
		"body": "\\mathrm{F} \\qty($1)$0"
	},

	"Probability Generating Function": {
		"prefix": "\\G",
		"body": "\\mathrm{G} \\qty($1)$0"
	},

	"Moment Generating Function": {
		"prefix": "\\M",
		"body": "\\mathrm{M} \\qty($1)$0"
	},

	"Sum": {
		"prefix": "\\sum",
		"body": "\\sum_{$1}^{$2} $0"
	},

	"Integral": {
		"prefix": "\\int",
		"body": "\\int_{$1}^{$2} $3 \\dd{$4}$0"
	},

	"Limit": {
		"prefix": "\\lim",
		"body": "\\lim_{$1} $0"
	},

	"Auto-sized ()": {
		"prefix" : "()",
		"body": "\\qty($1)$0"
	},

	"Auto-sized {}": {
		"prefix" : "{}",
		"body": "\\qty{$1}$0"
	},

	"Auto-sized []": {
		"prefix" : "[]",
		"body": "\\qty[$1]$0"
	},

	"Evaluation": {
		"prefix": "\\eval",
		"body": "\\eval[$1|_{$2}^{$3}$0"
	},

	"Equation $$": {
		"prefix": "$$",
		"body": "$$1$$0"
	},

	"Equation []": {
		"prefix" : "\\[]",
		"body": "\\[ $1 \\]$0"
	},

	"Subscript": {
		"prefix": "_{}",
		"body": "_{$1}$0"
	},

	"Superscript": {
		"prefix": "^{}",
		"body": "^{$1}$0"
	},

	"Complement": {
		"prefix": "\\complement",
		"body": "^{\\mathrm{c}}$0"
	},

	"Real Part": {
		"prefix": "\\Re",
		"body": "\\Re{$1}$0"
	},
}